  {{#addrealtor}}
    <form action="/addrealtor">
      <div class="row">
        <div class="col">
          <label for="firstname" class="form-label">First Name</label>
          <input type="text" class="form-control {{errorClasses.firstnameClass}}" value="{{formdata.firstname}}" name="firstname">
        </div>
        <div class="col">
          <label for="lastname" class="form-label">Last Name</label>
          <input type="text" class="form-control {{errorClasses.lastnameClass}}" value="{{formdata.lastname}}" name="lastname">
        </div>
      </div>
      <div class="row">
        <div class="col">
          <label for="specialties" class="form-label">Specialties</label>
          <input type="text" class="form-control {{errorClasses.specialtiesClass}}" value="{{formdata.specialties}}" name="specialties">
        </div>  
        <div class="col">
          <label for="likes" class="form-label">Likes</label>
          <input type="text" class="form-control {{errorClasses.likesClass}}" value="{{formdata.likes}}" name="likes">
        </div>
      </div>
      <input type="submit" class="btn btn-primary">
    </form>

  {{/addrealtor}}
  {{#updaterealtor}}
    <form action="/updaterealtor/{{updateid}}">
      <div class="row">
        <div class="col">
          <label for="firstname" class="form-label">First Name</label>
          <input type="text" class="form-control {{errorClasses.firstnameClass}}" value="{{formdata.firstname}}" name="firstname">
        </div>
        <div class="col">
          <label for="lastname" class="form-label">Last Name</label>
          <input type="text" class="form-control {{errorClasses.lastnameClass}}" value="{{formdata.lastname}}" name="lastname">
        </div>
      </div>
      <div class="row">
        <div class="col">
          <label for="specialties" class="form-label">Specialties</label>
          <input type="text" class="form-control {{errorClasses.specialtiesClass}}" value="{{formdata.specialties}}" name="specialties">
        </div>  
        <div class="col">
          <label for="likes" class="form-label">Likes</label>
          <input type="text" class="form-control {{errorClasses.likesClass}}" value="{{formdata.likes}}" name="likes">
        </div>
      </div>
      <input type="submit" class="btn btn-primary">
    </form>
  {{/updaterealtor}}
{{#errors}}
  <ul class="alert alert-danger" role="alert" id="errorlist">
    <li id="firstnameerror" style="display: block;">{{errors.firstname}}</li>
    <li id="lastnameerror" style="display: block;">{{errors.lastname}}</li>
    <li id="specialtieserror" style="display: block;">{{errors.specialties}}</li>
    <li id="likeserror" style="display: block;">{{errors.likes}}</li>
  </ul>
{{/errors}}
</body>
</html>